          "scope": "resource",
          "default": "#4bb14f",
          "description": "Color for the low complexity level",
          "type": "string"
        },
        "codemetrics.basics.ComplexityColorNormal": {
          "scope": "resource",
          "default": "#ffc208",
          "description": "Color for the normal complexity level",
